config_version=5

[application]

config/name="Paseo Catatumbo"
run/main_scene="res://Scenes/Menu/Game_Intro.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

CyclopsAutoload="*res://addons/cyclops_level_builder/cyclops_global_scene.tscn"
XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"

[display]

window/size/mode=3
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Paseo Catatumbo"

[editor_plugins]

enabled=PackedStringArray("res://addons/cyclops_level_builder/plugin.cfg")

